  "version": "1.0.0",
  "type": "module",
  "description": "React + Hono.js + Postgres application",
  "scripts": {
    "dev": "docker compose up -d & concurrently \"vite\" \"tsx watch server/index.ts\"",
    "server": "tsx server/index.ts",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "test": "vitest --run",
    "generate": "drizzle-kit generate",
    "migrate": "tsx server/db/migrate.js",
    "studio": "drizzle-kit studio --port 3333 --verbose"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ]
  },
