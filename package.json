    "@fontsource/merriweather": "^5.2.6",
    "@fontsource/merriweather-sans": "^5.2.5",
    "@hono/node-server": "^1.4.0",
    "@kylemathews/optimistic": "^0.0.8",
    "@radix-ui/react-toast": "^1.2.6",
